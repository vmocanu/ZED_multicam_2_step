{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ZED Capture Mode",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cpp/build/ZED_SpatialMappingFusion",
            "args": [
                "capture",
                "2.json",
                "--duration",
                "20"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/cpp",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "ZED Fusion Mode",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cpp/build/ZED_SpatialMappingFusion",
            "args": [
                "fusion",
                "2.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/cpp",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
