{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ZED Capture Mode",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cpp/build/ZED_SpatialMappingFusion",
            "args": [
                "capture",
                "2.json",
                "--duration",
                "20"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/cpp",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "/usr/local/zed/lib:/usr/local/cuda/lib64:${env:LD_LIBRARY_PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "additionalSOLibSearchPath": "/usr/local/zed/lib:/usr/local/cuda/lib64",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set disassembly flavor to intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "ZED Fusion Mode",
