{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ZED Capture Mode (Release)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-release/ZED_SpatialMappingFusion",
            "args": [
                "capture",
                "2.json",
                "--duration",
                "30",
                "--depth-mode",
                "neural_none"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/cpp",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "/usr/local/zed/lib:/usr/local/cuda/lib64:${env:LD_LIBRARY_PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "additionalSOLibSearchPath": "/usr/local/zed/lib:/usr/local/cuda/lib64",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Suppress thread creation/exit messages",
                    "text": "-gdb-set print thread-events off",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Release",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "ZED Fusion Mode (Release)",
